{
  "expo": {
    "name": "Gavel Simulator",
    "slug": "gavel-sim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gavel.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gavel.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dqdzung.myapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gavel.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eb99eab7-47f9-4610-895e-4d8d0423f9e7"
      }
    },
    "owner": "dqdzung"
  }
}
